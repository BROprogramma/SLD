                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" maxOccurs="1" name="featureCollection" nillable="false" type="imsikb0101:FeatureCollectionIMSIKB0101PropertyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SLD_NewDecisionPropertyType">
        <xs:sequence minOccurs="1">
            <xs:element ref="sldcom:SLD_NewDecision"/>
        </xs:sequence>
    </xs:complexType>

